<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="ITGCoTax\Core\Content\TaxProvider\TaxProviderDefinition">
            <tag name="shopware.entity.definition" entity="itg_tax_service_provider"/>
        </service>
        <service id="ITGCoTax\Core\Content\TaxLog\TaxLogDefinition">
            <tag name="shopware.entity.definition" entity="itg_taxjar_log"/>
        </service>
        <service id="ITGCoTax\Core\Content\Extension\TaxExtension">
            <tag name="shopware.entity.extension"/>
        </service>
        <service id="ITGCoTax\Core\Content\Extension\TaxExtensionDefinition">
            <tag name="shopware.entity.definition" entity="itg_tax_provider" />
        </service>
        <service id="ITGCoTax\Core\Checkout\Cart\Collector\AddTaxCollector">
            <argument type="service" id="product_price.repository" />
            <argument type="service" id="tax.repository" />
            <argument type="service" id="itg_tax_service_provider.repository" />
            <argument type="service" id="itg_taxjar_log.repository" />
            <argument type="service" id="product.repository" />
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="Shopware\Core\Checkout\Cart\Price\QuantityPriceCalculator"/>
            <argument type="service" id="cache.object"/>
            <tag name="shopware.cart.processor" priority="4500" />
        </service>
        <service id="ITGCoTax\Core\TaxJar\Calculator" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="itg_taxjar_log.repository" />
            <argument type="service" id="product.repository" />
            <argument type="service" id="cache.object"/>
        </service>
        <service id="ITGCoTax\Storefront\Controller\TestConnectionController" public="true">
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>
        <service id="ITGCoTax\Storefront\Controller\ExportLogController" public="true">
            <argument type="service" id="itg_taxjar_log.repository" />
        </service>
        <service id="ITGCoTax\Core\TaxJar\Order\TransactionSubscriber">
            <tag name="kernel.event_subscriber"/>
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="itg_taxjar_log.repository" />
            <argument type="service" id="order.repository" />
            <argument type="service" id="product.repository" />
            <argument type="service" id="country.repository" />
            <argument type="service" id="country_state.repository" />
            <argument type="service" id="order_transaction.repository" />
        </service>
        <service id="ITGCoTax\Service\ScheduledTask\CleanLogTask">
            <tag name="shopware.scheduled.task" />
        </service>
        <service id="ITGCoTax\Service\ScheduledTask\CleanLogTaskHandler">
            <argument type="service" id="scheduled_task.repository"/>
            <argument type="service" id="itg_taxjar_log.repository" />
            <tag name="messenger.message_handler" />
        </service>
    </services>
</container>
