                    "$ref": "#/components/parameters/criteriaPage"
                },
                {
                    "name": "limit",
                    "in": "query",
                    "description": "Number of items per result page. If not set, the limit will be set according to the default products per page, defined in the system settings.",
                    "schema": {
                        "type": "integer",
                        "minimum": 0
                    }
                },
                {
                    "$ref": "#/components/parameters/criteriaTerm"
                },
                {
                    "$ref": "#/components/parameters/criteriaFilter"
                },
                {
                    "$ref": "#/components/parameters/criteriaIds"
